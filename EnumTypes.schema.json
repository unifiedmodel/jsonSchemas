{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://unifiedmodel.github.io/jsonSchemas/EnumTypes.schema.json",
    "type": "object",
    "title": "List of custom enumerated types (EnumTypes)",
    "properties": {
        "enumSize": {
            "enum": [
                "Micro",
                "Small",
                "Medium",
                "Large",
                "Unknown"
            ]
        },
        "enumScale": {
            "enum": [
                "Units",
                "Thousands",
                "Millions"
            ]
        },
        "enumPeriod": {
            "enum": [
                "Daily",
                "Weekly",
                "Monthly",
                "BiMonthly",
                "Quarterly",
                "SixMonthly",
                "Yearly"
            ]
        },
        "enumPublicRecordsGroupType": {
            "enum": [
                "Bankruptcy, Insolvency and Restructuring",
                "Judicial and Administrative",
                "Defaulted Payments",
                "Other"
            ]
        },
        "enumEmployeesEvolutionTypeGroup": {
            "enum": [
                "By type of contract",
                "By job title",
                "By gender",
                "Paid vs unpaid staff",
                "By type of working day"
            ]
        },
        "enumSalesEvolutionTypeGroup": {
            "enum": [
                "By location",
                "By activity",
                "By product",
                "By customers"
            ]
        },
        "enumFinancialStatementsSection": {
            "enum": [
                "Financial summary",
                "Balance sheet",
                "Income statement",
                "Cash Flow statement",
                "Statement of changes in equity",
                "Ratios",
                "Sectoral comparative"
            ]
        },
        "enumFrequencyDataType": {
            "enum": [
                "Percent",
                "Percentage",
                "Ratio",
                "Days",
                "Weeks",
                "Months",
                "Years"
            ]
        },
        "enumConnotation": {
            "enum": [
                "Positive",
                "Negative",
                "Neutral"
            ]
        },
        "enumBusinessType": {
            "enum": [
                "Self employed",
                "Affiliated company",
                "Group subsidiary",
                "Parent company"
            ]
        }
    }
}