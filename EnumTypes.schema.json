{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://unifiedmodel.github.io/jsonSchemas/EnumTypes.schema.json",
    "type": "object",
    "title": "List of custom enumerated types (EnumTypes)",
    "properties": {
        "enumSize": {
            "enum": [
                "Micro",
                "Small",
                "Medium",
                "Large",
                "Unknown"
            ]
        },
        "enumScale": {
            "enum": [
                "Units",
                "Thousands",
                "Millions"
            ]
        },
        "enumPeriod": {
            "enum": [
                "Daily",
                "Weekly",
                "Monthly",
                "BiMonthly",
                "Quarterly",
                "SixMonthly",
                "Yearly"
            ]
        },
        "enumPublicRecordsGroupType": {
            "enum": [
                "BankruptcyInsolvencyRestructuring",
                "JudicialAdministrative",
                "DefaultedPayments",
                "Other"
            ]
        },
        "enumEmployeesEvolutionTypeGroup": {
            "enum": [
                "ByTypeOfContract",
                "ByJobTitle",
                "ByGender",
                "PaidVsUnpaidStaff",
                "ByTypeOfWorkingDay"
            ]
        },
        "enumSalesEvolutionTypeGroup": {
            "enum": [
                "ByLocation",
                "ByActivity",
                "ByProduct",
                "ByCustomers"
            ]
        },
        "enumFinancialStatementsSection": {
            "enum": [
                "FinancialSummary",
                "BalanceSheet",
                "IncomeStatement",
                "CashFlowStatement",
                "StatementOfChangesInEquity",
                "Ratios",
                "SectoralComparative"
            ]
        },
        "enumFrequencyDataType": {
            "enum": [
                "Percent",
                "Percentage",
                "Ratio",
                "Days",
                "Weeks",
                "Months",
                "Years"
            ]
        },
        "enumConnotation": {
            "enum": [
                "Positive",
                "Negative",
                "Neutral"
            ]
        },
        "enumBusinessType": {
            "enum": [
                "SelfEmployed",
                "AffiliatedCompany",
                "GroupSubsidiary",
                "ParentCompany"
            ]
        }
    }
}